{
    "name": "black/common-bundle",
    "type": "symfony-bundle",
    "description": "Symfony BlackCommonBundle",
    "keywords": ["Symfony2", "Bundle", "Black"],
    "homepage": "http://www.lablackroom.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Balmes",
            "email": "abalmes@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "symfony/framework-bundle": "~2.3",
        "symfony/form": "~2.3",
        "doctrine/doctrine-bundle": "1.2.*",
        "jms/security-extra-bundle": "1.5.*",
        "jms/di-extra-bundle": "1.4.*",
        "sonata-project/intl-bundle": "2.1.*",
        "stof/doctrine-extensions-bundle": "1.1.*"
    },
    "suggest": {
        "doctrine/mongodb-odm": "If you want to use MongoDB",
        "doctrine/mongodb-odm-bundle": "If you want to use MongoDB"
    },
    "autoload": {
        "psr-0": { "Black\\Bundle\\CommonBundle": "" }
    },
    "target-dir": "Black/Bundle/CommonBundle"
}
